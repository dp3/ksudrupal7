/**
 * @file
 * Navigation Styling
 */


/*
 * The active item in a Drupal menu
 */
li a.active {
}

/*
 * Navigation bar
 */
#navigation {
   float:center;
   width:100%;
   background:#512888;
   border-bottom:4px solid #ecd391;
   overflow:hidden;
   position:relative;
}

.region-navigation {
}

/*
 * Primary and Secondary menu links
 */
#main-menu {
}

#secondary-menu {
}

#secondary-menu a:link, 
#secondary-menu a:visited {
}

#secondary-menu a:hover, 
#secondary-menu a:focus {
}
/*
* Breadcrumbs
*/

#breadcrumb {
  margin-left: 220px;
  font-size: 9px;
}

#breadcrumb a:link,
#breadcrumb a:visited,
#breadcrumb a:active,
#breadcrumb a:hover {
  color: #666666;
  z-index: 999;
}

/*
* Navigation Menu
*/
.block {
}

.block-title {
display:none;
}


.block ul {
//  list-style: none;
}

.block ul.menu,ul.nice-menu {
  padding-left: 0;

}

.block .content {

}


