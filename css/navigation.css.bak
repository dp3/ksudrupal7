#block-nice-menus-1 {
  float: left;
  position: relative;
  left: 50%;
}

#block-nice-menus-1 #nice-menu-1 {
  position: relative;
  float: left;
  left: -37%;
} 

.menu__item {
  z-index: 999;
}






/* Applies to all menus */
ul.menu {
     list-style-type: none;
     //margin-left: 180px;
     //margin-right: 180px;
    position:relative;
    width:100%;
}

/*Applies to all menu li's */
ul.menu li {
  //display:block;
        float: left;
  //list-style-type: none;
  //list-style-image: none;
}

ul.menu li ul li{
    display:block;
}
/* Applies to all menu li a's */
ul.menu li a {
  background-color: #512888;
  order-left: 1px solid #411977;
  display: block;
 // color: #FFFFFF;
//  text-decoration: none;
  line-height:31px;
  height:36px;
  //list-style: none;
  font-size: 0.75rem; /* 1em = 13px: 0.8461em * 13px = 11px */
  font-weight: 700;
  padding: 4px 20px 3px 20px;  //top right bottom left 
  border: 5px solid black;

//  border-bottom: solid 5px #ecd391;
//  position: relative;
//      margin: auto;
//  margin: 0 -9999rem;
}


ul.menu a:hover,
ul.menu a:active,
ul.menu ul.menu a:hover,
ul.menu ul.menu a:active,
ul.menu li.menu a:hover,
ul.menu li.menu a:active {
  //background-color: #ecd391;
 // color:#000000;
    z-index: 999;
 display:table;
}

ul.menu ul {
  //display: none;
  position: absolute;
  top: 100%;
  left: 0;
  background: black;
  padding: 0;
 z-index: 999; 
    border: 5px solid blue;
}

.content > ul.menu > li.first > a {
  border-radius: 8px 8px 0 0;
}

.content > ul.menu > li.last > a { 
  border-radius: 0 0 8px 8px;
}

ul.menu li ul.menu li,
ul.menu li ul.menu li:hover, 
ul.menu li ul.menu li:active{ 
        border: 5px solid brown;
}

ul.menu li:hover ul {
  display:block;
  position:absolute;
  z-index:999;
}


ul.menu li ul li {
  display:block;
  z-index:999;
}

ul.menu li ul li a {
  display:block;
  z-index:999;
}

ul.menu li ul.menu li a {
  display:block;
  z-index:999;
  color: red;
  margin-left: 20px;
  border-bottom: none;
  background-color: #eeede8;
  border: 5px solid yellow;
}

ul.menu li ul.menu li ul.menu li a {
  margin-left: 40px;
}




