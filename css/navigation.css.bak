 * Menu blocks
 */
.block-menu {
}

/*
 * "Menu block" blocks. See http://drupal.org/project/menu_block
 */
.block-menu_block {
}

/*
 * Pager
 */

.pager-current { 
  font-weight: bold;
  background-color: #cbebf9;
}

.content > ul.menu > li.first > a {
  border-radius: 8px 8px 0 0;
}

.content > ul.menu > li.last > a { 
  border-radius: 0 0 8px 8px;
}
